<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="ScratchCard">
        <attr name="scratchDrawable" format="reference|color" />
        <attr name="scratchWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TemplateView" tools:ignore="ResourceName">
        <attr name="gnt_template_type" format="reference" />
        <attr name="gnt_template_min_height" format="integer" />
    </declare-styleable>


    <declare-styleable name="RoundedImage">
        <attr name="rounded_radius" format="float" />
    </declare-styleable>
    <declare-styleable name="CircleImage">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="add_shadow" format="boolean" />
        <attr name="shadow_color" format="color" />
        <attr name="shadow_radius" format="float" />
    </declare-styleable>

</resources>